PROC SORT DATA=SUA_TABELA;
    BY COLUNA_ORDEM_1 COLUNA_ORDEM_2;
RUN;
DATA NOVA_TABELA;
    SET SUA_TABELA;
    BY COLUNA_COMUM_1 COLUNA_COMUM_2;

    NOME_NOVA_COLUNA = LAG1(NOME_COLUNA_ANTIGA);

    IF NOT FIRST.NOME_COLUNA_COMUM THEN 
        NOME_NOVA_COLUNA = REGRA QUE QUER USAR;
    ELSE 
        VALOR_DIFF = .; /* ou VALOR_DIFF = 0; não há mês anterior */
    
    DROP NOME_COLUNA_ANTIGA;
RUN;
